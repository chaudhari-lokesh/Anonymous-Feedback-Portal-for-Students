{
  "buildCommand": "cd 'feedback portal' && npm run build",
  "outputDirectory": "feedback portal/dist",
  "installCommand": "cd 'feedback portal' && npm install",
  "env": {
    "NODE_ENV": "production"
  },
  "headers": [
    {
      "source": "/assets/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/index.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
